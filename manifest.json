  {
    "name": "Recipe Saver Chrome Extension",
    "version": "1.0",
    "description": "Save a recipe in Sugar Salt Butter",
    "permissions": [
        "storage",
        "tabs",
        "http://*/",
        "https://*/",
        "identity"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "oauth2": {
      "client_id": "CLIENT_ID",
      "scopes":["profile"]
    },
    "key": "APP_KEY",
    "manifest_version": 2
  }